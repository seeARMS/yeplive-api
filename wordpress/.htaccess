<Files ".htpasswd"> 
	deny from all 
</Files>

AuthType Basic   
AuthName "Private zone!" 
AuthUserFile  /var/www/yeplive/wordpress/.htpasswd   
require valid-user

<Files "xmlrpc.php"> 
	allow from all
	satisfy any
</Files>

Options -Indexes
ErrorDocument 404 /error/404.php

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress

RewriteRule ^([0-9]+)$ /?programId=$1 [L]